<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.serviceModel>
    <bindings>
      <wsDualHttpBinding>
        <binding name="ChatService">
          <security mode="None" />
        </binding>
      </wsDualHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://127.0.0.1:30000/ServiceChat" binding="wsDualHttpBinding"
        bindingConfiguration="ChatService" contract="ServiceReferenceChat.IServiceChat"
        name="ChatService">
        <identity>
          <dns value="localhost" />
        </identity>
      </endpoint>
    </client>
  </system.serviceModel>
</configuration>